{
  "name": "cats",
  "version": "0.0.0",
  "description": "Catalog of cats",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "concurrently -g 'npm run lint:es' 'npm run lint:style' 'npm run lint:format'",
    "lint:fix": "concurrently -g 'npm run lint:es:fix' 'npm run lint:style:fix' 'npm run lint:format:fix'",
    "lint:es": "ng run cats:eslint",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint:style": "ng run cats:stylelint",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:format": "prettier --check \"**/*.{js,json,scss,md,ts,html}\" --ignore-path .gitignore",
    "lint:format:fix": "prettier --write \"**/*.{js,json,scss,md,ts,html}\" --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lykoffant/cats.git"
  },
  "keywords": [
    "cats",
    "pet-project",
    "single-page-app",
    "angular"
  ],
  "author": "Anton Lykov",
  "bugs": {
    "url": "https://github.com/lykoffant/cats/issues"
  },
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.3",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "~15.1.3",
    "@angular/compiler-cli": "^15.1.0",
    "@bmatei/stylelint-builder-angular": "^1.1.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^2.3.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "~4.9.4"
  }
}
